#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,x,y,z,q,a,b,c,d;
    for(i=1000; i<=9999; i++){
    x=i%10;
    y=(i/10)%10;
    z=(i/100)%10;
    q=i/1000;
    a=pow(x,x);
    b=pow(y,y);
    c=pow(z,z);
    d=pow(q,q);
    if(i==a+b+c+d){
        printf("%d", i);
    }
    }

    return 0;
}
