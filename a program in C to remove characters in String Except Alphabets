#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int main()
{
    char cumle[100];
    printf("Cumle: ");
    fgets(cumle,100,stdin);
    int i;
    for(i=0; i<strlen(cumle); i++){
        if(isalpha(cumle[i])){
            printf("%c", cumle[i]);
        }
    }
    return 0;
}
