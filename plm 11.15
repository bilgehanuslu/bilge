#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,a[5],b[5],toplam=0;
    printf("1. diziyi giriniz: ");
    for(i=0; i<5; i++){
        scanf("%d", &a[i]);
    }
    printf("2. diziyi giriniz: ");
    for(i=0; i<5; i++){
        scanf("%d", &b[i]);
    }
    printf("Elemanlari toplami olan dizi: ");
    for(i=0; i<5; i++){
        toplam=a[i]+b[i];
        printf("%d ", toplam);
        toplam=0;
    }
    return 0;
}
